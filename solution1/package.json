  "main": "index.js",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "webpack-dev-server --open --config ./config/webpack.dev.js",
    "start:prod": "webpack-dev-server --open --config ./config/webpack.prod.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:dev": "rimraf dist && webpack --config ./config/webpack.dev.js",
    "build:prod": "rimraf dist && webpack --config ./config/webpack.prod.js",
    "build:perf": "rimraf dist && webpack --config ./config/webpack.perf.js",
    "test": ""
  },
